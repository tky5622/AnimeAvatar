{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/*",
    "src/components/**/*.tsx",
    "src/components/**/*.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "strict": false,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  }
}
